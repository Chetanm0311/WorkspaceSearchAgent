# Server configuration
PORT=8000
ENVIRONMENT=development
LOG_LEVEL=INFO

# Authentication settings
AUTH_ENABLED=false
DESCOPE_BASE_URL=https://api.descope.com
DESCOPE_PROJECT_ID=development_project_id
DESCOPE_API_KEY=development_api_key

# Security settings
CEQUENCE_ENABLED=false
CEQUENCE_API_KEY=development_cequence_key

# Google Drive API
GOOGLE_DRIVE_PRODUCTION=true
GOOGLE_CREDENTIALS_PATH=credentials.json
GOOGLE_TOKEN_PATH=token.json

# Notion API
NOTION_API_KEY=development_notion_key

# Slack API
SLACK_API_TOKEN=development_slack_token

# Confluence API
CONFLUENCE_USERNAME=development_confluence_user
CONFLUENCE_API_TOKEN=development_confluence_token

# Cache settings
CACHE_ENABLED=true
SEARCH_CACHE_TTL=300
DOCUMENT_CACHE_TTL=600
UPDATES_CACHE_TTL=300
